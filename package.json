{
  "name": "@jozsefDevs/component-frontend-blueprint",
  "author": {
    "email": "gubijo@gmail.com",
    "name": "Jozsef Gubicza"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "unpkg": "dist/index.js",
  "files": [
    "dist"
  ],
  "version": "0.0.0-development",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "test": "yarn type-check && yarn lint",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --progress --mode=production",
    "build": "rimraf dist && yarn build:types && yarn build:js",
    "lint": "eslint src/**/*{.ts,.tsx} --format=pretty",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "build-storybook -c .storybook -o .storybook-build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-a11y": "5.3.19",
    "@storybook/addon-backgrounds": "5.3.19",
    "@storybook/addon-centered": "5.3.19",
    "@storybook/addon-info": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@types/react-dom": "16.9.8",
    "@types/storybook__addon-a11y": "5.1.2",
    "@types/storybook__addon-centered": "5.2.1",
    "@types/storybook__addon-info": "5.2.1",
    "@types/storybook__addon-knobs": "5.2.1",
